<% include ../partials/header %>
    <form action="/products/insert" method="post">
        <div class="form-group">
            <label for="">ID</label>
            <input type="number" class="form-control"  name="id" id="" aria-describedby="helpId" placeholder="">
            <small id="helpId" class="form-text text-muted">Help text</small>
        </div>

        <div class="form-group">
            <label for="">Title</label>
            <input type="text" class="form-control" name="title" id="" aria-describedby="helpId" placeholder="">
            <small id="helpId" class="form-text text-muted">Help text</small>
        </div>
        <div class="form-group">
            <label for="">Price</label>
            <input type="number" class="form-control" name="price" id="" aria-describedby="helpId" placeholder="">
            <small id="helpId" class="form-text text-muted">Help text</small>
        </div>

        <button type="reset" class="btn btn-secondary">Cancel</button>
        <a class="btn btn-primary" href="/product/insert" role="button">Add</a>
        
        
    </form>
    <% include ../partials/footer %>