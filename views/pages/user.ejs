<% include ../partials/header %>
    <table class="table">
        <thead>
            <tr>
                <th>ID</th>
                <th>Name</th>
                <th>Price</th>
                <th><a class="btn btn-success" href="/insert_user" role="button">Add New</a></th>
                
            </tr>
        </thead>
        <tbody>
            <% user.forEach(function(user){%>
                <tr>
                    <td scope="row">
                        <%=user.id %>
                    </td>
                    <td>
                        <%=user.email %>
                    </td>
                    <td>
                        <%=user.password %>
                    </td>
                    <td>
                    <a class="btn btn-primary" href="/user/<%= user.id%>" role="button">Edit</a>
                </td>
                </tr>
                <%}); %>
        </tbody>
    </table>

    <% include ../partials/footer %>